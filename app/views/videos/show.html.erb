<div class="container"> 
    <div class="row">
        <h1>videos</h1>
        <%= @videos.find_each do |x| %>
            <div class="col col-sm-4 col-md">
                <div id="video" class="center">
                    <% puts "#{x.link}" %>
                    <iframe width="380" height="300" id="video"
                        src=<%= "https://www.youtube.com/embed/#{x.link}" %> frameborder="0" allowfullscreen>
                    </iframe>
                    <h4>Stick Your Video Here <!-- add id=""title_p" when you will have embeded ruby --></h4>
                </div>
            </div>
        <% end %> 
    </div>
</div>